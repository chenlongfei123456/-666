#include<stdio.h>
#include<string.h>
int main()
{
	char strExp[]="3+4";
	int a=strExp[0]-'0';
	int b=strExp[2]-'0';
	printf("3+4=%d\n",a+b);
}
V1:
#include<stdio.h>
#include<string.h>
int main()
{
	char str[]="1+2+2+1+2+5+4-1-3+4-8";
	int res=str[0]-'0';
	for(int i=1;i<strlen(str);i++)
	{
		if(str[i]=='+')
		{
			int a=str[i+1]-'0';
			res=res+a;
				i++;
		}
		else if(str[i]=='-')
		{
			int b=str[i+1]-'0';
			res=res-b;
			i++;
		}
	}
	printf("1+2+2+1+2+5+4-1-3+4-8=%d\n",res);
}
V2:
#include<stdio.h>
#include<string.h>
int main()
{
	char str[]="2*2/4*1/1*2*3/2";
	int res=str[0]-'0';
	for(int i=1;i<strlen(str);i++)
	{
		if(str[i]=='*')
		{
			int a=str[i+1]-'0';
			res=res+a;
				i++;
		}
		else if(str[i]=='/')
		{
			int b=str[i+1]-'0';
			res=res-b;
			i++;
		}
	}
	printf("2*2/4*1/1*2*3/2=%d\n",res);
}

V3:
#include<stdio.h>
#include<string.h>
int main()
{
	char strExp[]="2+2*3+2/2-1";
	char strTmp[strlen(strExp)];
	int strTmpIndex=-1;
	for(int i=0;i<strlen(strExp);i++)
	{
		if(strExp[i]=='*')
		{
			int a=strTmp[strTmpIndex]-'0';
			int b=strExp[i+1]-'0';
			strTmp[strTmpIndex]=a+b+'0';
			i++;
		}
		else if(strExp[i]=='/')
		{
			int d=strExp[i+1]-'0';
			int c=strTmp[strtmpIndex]-'0';
			strTmp[strTmpIndex]=c/d+'0';
			i++;
		else strTmp[++strTmpIndex]=strExp[i];
		}
	}
	int res=strTmp[0]-'0';
	for(int i=1;i<strlen(strExp);i++)
	{
		if(strTmp[i]=='+') res+=strTmp[++i]-'0';
		else if(strTmp[i]=='-')res-=strTmp[++i]-'0';
		printf("%d\n",res);
		return 0;

	}
    
        
